<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>mavenproject5</display-name>
    <servlet>
        <servlet-name>AddEmployeeServlet</servlet-name>
        <servlet-class>com.mycompany.servlet.AddEmployeeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeReportServlet</servlet-name>
        <servlet-class>com.mycompany.servlet.EmployeeReportServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddEmployeeServlet</servlet-name>
        <url-pattern>/AddEmployeeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DisplayEmployeeServlet</servlet-name>
        <servlet-class>com.mycompany.servlet.DisplayEmployeeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayEmployeeServlet</servlet-name>
        <url-pattern>/DisplayEmployeeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeReportServlet</servlet-name>
        <url-pattern>/EmployeeReportServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>DisplayEmployeeServlet</welcome-file>
    </welcome-file-list>
</web-app>
